<?xml version="1.0" encoding="UTF-8"?>

<XMBML version="1.0">
	<View id="root">
		<Attributes>
			<Table key="cfw_settings_en">
				<Pair key="focus_priority"><String>1</String></Pair>
			</Table>
		</Attributes>
		<Items>
			<Query
				class="type:x-xmb/folder-pixmap"
				key="cfw_settings_en"
				src="xmb://localhost/%flash/xmb/cfw_settings.xml#seg_cfw_settings"
				/>
			<Query
				 class="type:x-xmb/folder-pixmap"
				 key="tcxs_project"
				 src="xmb://localhost/dev_hdd0/game/TCXSPROJECT/USRDIR/tcxsproject_xmb.xml#tcxs_xmb_main"
				 />
			<Query
				class="type:x-xmb/folder-pixmap"
				key="seg_ps3xploit_site"
				attr="seg_ps3xploit_site"
				src="#seg_ps3xploit_site"
				/>
			<Query
				class="type:x-xmb/folder-pixmap"
				key="seg_manual"
				src="xmb://localhost/%flash/xmb/category_network.xml#seg_manual"
				/>
			<Query
				class="type:x-xmb/folder-pixmap"
				key="seg_premo"
				src="sel://localhost/nopremo?category_network.xml#seg_premo"
				/>
			<Query 
				class="type:x-xmb/folder-pixmap" 
				key="seg_browser" 
				attr="seg_browser" 
				src="xmb://localhost/%flash/xmb/category_network.xml#seg_browser"
				/>
			<Query
				class="type:x-xmb/folder-pixmap"
				key="seg_kensaku"
				src="xmb://localhost/%flash/xmb/category_network.xml#seg_kensaku"
				/>
			<Query
				class="type:x-xmb/folder-pixmap"
				key="seg_dlctrl"
				src="ese://localhost/query?type=download_control"
				/>
			<Query
				class="type:x-xmb/folder-pixmap"
				key="seg_gameexit"
				src="sel://localhost/ingame?path=category_network.xml#seg_gameexit&type=network"
				/>
			<Query
				class="type:x-xmb/folder-pixmap"
				key="gameDir"
				attr="gameDir"
				src="xcb://localhost/query?limit=2048&table=MMS_MEDIA_TYPE_HDD&sort=-Game:Common.stat.rating-Game:Common.timeCreated&cond=Aa+Game:Common.title+Ae+Game:Game.category CB"
			/>
		</Items>
	</View>

	<View id="seg_ps3xploit_site">
		<Attributes>
			<Table key="ps3xploit_site">
				<Pair key="icon_rsc"><String>tex_browser</String></Pair>
				<Pair key="icon_notation"><String>WNT_XmbItemBrowser</String></Pair>
				<Pair key="title"><String>Team PS3Xploit</String></Pair>
				<Pair key="module_name"><String>webrender_plugin</String></Pair>
				<Pair key="module_action"><String>http://ps3xploit.com</String></Pair>
 			</Table>
		</Attributes>
		<Items>
			<Item class="type:x-xmb/module-action" key="ps3xploit_site" attr="ps3xploit_site" />
		</Items>
	</View>

</XMBML>
